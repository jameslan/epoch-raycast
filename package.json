{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "epoch-time-converter",
  "title": "Epoch Time Converter",
  "description": "Convert between epoch time (unix timestamp) and human readable time",
  "icon": "command-icon.png",
  "author": "jameslan",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "convertCommand",
      "title": "epoch",
      "description": "Convert between epoch time (unix timestamp) and human readable time",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}